<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="1120.0" style="-fx-background-color: #97adc4;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.aykanberkesandal.VerwaltungspersonalController">
   <children>
      <ImageView fitHeight="250.0" fitWidth="250.0" layoutX="427.0" layoutY="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../admin.png" />
         </image>
      </ImageView>
      <Label layoutX="14.0" layoutY="5.0" text="Vpersonal">
         <font>
            <Font name="System Bold Italic" size="25.0" />
         </font>
      </Label>
      <TableView fx:id="verwaltungspersonalTable" layoutY="46.0" prefHeight="500.0" prefWidth="1120.0">
        <columns>
          <TableColumn fx:id="col_UID_verwaltungspersonal" prefWidth="160.0" text="VerwaltungspersonalID" />
            <TableColumn fx:id="col_vorname_verwaltungspersonal" prefWidth="160.0" text="Vorname" />
            <TableColumn fx:id="col_name_verwaltungspersonal" prefWidth="160.0" text="Name" />
            <TableColumn fx:id="col_telefonnummer_verwaltungspersonal" prefWidth="160.0" text="Telefonnummer" />
            <TableColumn fx:id="col_adresse_verwaltungspersonal" prefWidth="160.0" text="Adresse" />
            <TableColumn fx:id="col_ID_verwaltungspersonal" prefWidth="160.0" text="ID" />
            <TableColumn fx:id="col_SVN_verwaltungspersonal" prefWidth="160.0" text="SVN" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="14.0" layoutY="575.0" mnemonicParsing="false" onAction="#loadAddPopup" prefHeight="50.0" prefWidth="150.0" style="-fx-background-color: #268cb4; -fx-border-color: black; -fx-border-width: 3px;" text="Hinzufüge" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="delButton" layoutX="198.0" layoutY="575.0" mnemonicParsing="false" onAction="#loadDelPopup" prefHeight="50.0" prefWidth="150.0" style="-fx-background-color: #268cb4; -fx-border-color: black; -fx-border-width: 3px;" text="Lösche" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="updtButton" layoutX="383.0" layoutY="575.0" mnemonicParsing="false" onAction="#loadUpdtPopup" prefHeight="50.0" prefWidth="150.0" style="-fx-background-color: #268cb4; -fx-border-color: black; -fx-border-width: 3px;" text="Aktualisiere" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button layoutX="162.0" layoutY="6.0" mnemonicParsing="false" onAction="#clickListVerwaltungspersonal" prefHeight="34.0" prefWidth="277.0" style="-fx-background-color: #268cb4; -fx-border-color: black; -fx-border-width: 3px;" text="schalte die Liste der Verwaltungspersonal" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
